<html lang="en">
  <head>
      <meta charset="utf-8">
      <link   href="/css/bootstrap.min.css" rel="stylesheet">
      <script src="/js/bootstrap.min.js"></script>
  </head>
  <body>
	   <h1>TOTAL FITAS</h1>
      <table class="table table-striped table-bordered">
        <thead>
          <tr>
            <th>Projeto</th>
            <th>Total Fitas Gravadas</th>
            <th>REPOSITORIO EXTERNO</th>
            <th>REPOSITORIO LOCAL</th>
            <th>TOTAL FITAS SCRATCH</th>
            <th>TOTAL FITAS</th>
          </tr>
        </thead>
        <tbody>
          <tr for (info in data)>
              <th scope="row">${info.group}</th>
              <td>${info.total_remote +info.total_local}</td>
              <td>${info.total_remote}</td>
              <td>${info.total_local}</td>
              <td>${info.total_scratch}</td>
              <td>${info.total}</td>
          </tr>
        </tbody>
      </table>
  </body>
</html>
